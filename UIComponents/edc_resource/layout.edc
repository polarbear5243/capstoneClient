group { name: "progressbar_bg_layout";
   parts {
      part { name: "base";
         type: RECT;
         scale: 1;
         description { state: "default" 0.0;
            color: 250 250 250 255;
         }
      }
      part { name: "elm.swallow.content";
         type: SWALLOW;
         scale: 1;
         description { state: "default" 0.0;
            rel1.to: "pad_left";
            rel2.to: "pad_right";
            rel1.relative: 1 1;
            rel2.relative: 0 0;
         }
      }
      spacer { "pad_left";
         scale: 1;
         desc { "default";
            rel1.to: "base";
            rel2.to: "base";
            rel1.relative: 0 0;
            rel2.relative: 0 0;
            align: 0 0;
            min: 32 0;
         }
      }
      spacer { "pad_right";
         scale: 1;
         desc { "default";
            rel1.to: "base";
            rel2.to: "base";
            rel1.relative: 1 1;
            rel2.relative: 1 1;
            align: 1 1;
            min: 32 0;
         }
      }
   }
}
