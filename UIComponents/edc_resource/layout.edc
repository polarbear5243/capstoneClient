group { name: "white_bg_layout";
   parts {
      part { name: "base";
         type: RECT;
         scale: 1;
         description { state: "default" 0.0;
            color: 250 250 250 255;
         }
      }
      part { name: "elm.swallow.content";
         type: SWALLOW;
         scale: 1;
         description { state: "default" 0.0;
         }
      }
   }
}

group { name: "button_layout_1";
   parts {
      part { name: "padding.left.top";
         type: RECT;
         description { state: "default" 0.0;
            fixed: 1 1;
            align: 0.0 0.0;
            min: 32 21;
            max: 32 21;
            rel2.to: "base";
            rel2.relative: 0.0 0.0;
         }
      }
      part { name: "padding.right.bottom";
         type: RECT;
         description { state: "default" 0.0;
            fixed: 1 1;
            align: 1.0 1.0;
            min: 32 21;
            max: 32 21;
            rel1.to: "base";
            rel1.relative: 1.0 1.0;
         }
      }
      part { name: "base";
         type: RECT;
         scale: 1;
         description { state: "default" 0.0;
            min: 496 78;
            color: 0 0 0 0;
         }
      }
      part { name: "elm.swallow.content";
         type: SWALLOW;
         scale: 1;
         description { state: "default" 0.0;
            rel1.to: "padding.left.top";
            rel1.relative: 1.0 1.0;
            rel2.to: "padding.right.bottom";
            rel2.relative: 0.0 0.0;
         }
      }
   }
}

